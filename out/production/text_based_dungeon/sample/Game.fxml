<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<?import javafx.scene.image.ImageView?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Pane prefHeight="764.0" prefWidth="846.0">
         <children>
            <VBox prefHeight="764.0" prefWidth="846.0">
               <children>
                  <HBox prefHeight="568.0" prefWidth="846.0">
                     <children>
                        <VBox prefHeight="568.0" prefWidth="285.0">
                           <children>
                              <ImageView fx:id="playerImage" />
                           </children>
                        </VBox>
                        <VBox prefHeight="568.0" prefWidth="287.0">
                           <children>
                              <TextField fx:id="mainText" prefHeight="572.0" prefWidth="287.0">
                                 <opaqueInsets>
                                    <Insets />
                                 </opaqueInsets>
                              </TextField>
                           </children>
                        </VBox>
                        <VBox prefHeight="568.0" prefWidth="275.0" >
                           <children>
                              <ImageView fx:id="enemyImage" />
                           </children>
                        </VBox>
                     </children>
                  </HBox>
                  <HBox prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <Button fx:id="importDungeon" onAction="#importPressed" mnemonicParsing="false" prefHeight="79.0" prefWidth="255.0" text="Import Dungeon" />
                        <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="369.0" text="Button" />
                        <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="246.0" text="Button" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
      </Pane>
   </children>
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
   </rowConstraints>
</GridPane>
